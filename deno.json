{
    "tasks": {
        "clean": "rm -rf dist && echo 'Project cleaned! Dist folder deleted.'",
        "build": "deno run -A npm:prisma generate",
        "start": "deno run -A npm:prisma migrate deploy; deno run --env-file --allow-all src/app.ts",
        "dev": "deno run --env-file --allow-all --watch src/app.ts",
        "migrate:dev": "deno run -A npm:prisma migrate dev",
        "compile": "deno compile --allow-all --no-check src/app.ts"
    },
    "imports": {
        "@prisma/adapter-pg": "npm:@prisma/adapter-pg@^7.1.0",
        "@prisma/client": "npm:@prisma/client@^7.1.0",
        "@std/assert": "jsr:@std/assert@1",
        "@ts-rest/core": "npm:@ts-rest/core@^3.53.0-rc.1",
        "@ts-rest/express": "npm:@ts-rest/express@^3.53.0-rc.1",
        "bcrypt": "npm:bcrypt@^6.0.0",
        "cors": "npm:cors@^2.8.5",
        "dotenv": "npm:dotenv@^17.2.2",
        "dotenv-cli": "npm:dotenv-cli@^10.0.0",
        "express": "npm:express@^5.1.0",
        "jsonwebtoken": "npm:jsonwebtoken@^9.0.2",
        "pg": "npm:pg@^8.16.3",
        "pg-format": "npm:pg-format@^1.0.4",
        "prisma": "npm:prisma@^7.1.0",
        "zod": "npm:zod@^4.1.11"
    },
    "nodeModulesDir": "auto"
}
