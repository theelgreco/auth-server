{
    "tasks": {
        "build": "deno run -A npm:prisma generate",
        "start": "deno run -A npm:prisma migrate deploy; deno run --allow-all src/app.ts",
        "dev": "deno run --allow-all --watch src/app.ts",
        "migrate:dev": "deno run -A npm:prisma migrate dev",
        "compile": "deno compile --allow-all --no-check src/app.ts"
    },
    "imports": {
        "@prisma/client": "npm:@prisma/client@^6.16.2",
        "@std/assert": "jsr:@std/assert@1",
        "bcrypt": "npm:bcrypt@^6.0.0",
        "boolean": "npm:boolean@^3.2.0",
        "cors": "npm:cors@^2.8.5",
        "dotenv": "npm:dotenv@^17.2.2",
        "dotenv-cli": "npm:dotenv-cli@^10.0.0",
        "email-validator": "npm:email-validator@^2.0.4",
        "express": "npm:express@^5.1.0",
        "jsonwebtoken": "npm:jsonwebtoken@^9.0.2",
        "morgan": "npm:morgan@^1.10.1",
        "pg": "npm:pg@^8.16.3",
        "pg-format": "npm:pg-format@^1.0.4"
    },
    "nodeModulesDir": "auto"
}
